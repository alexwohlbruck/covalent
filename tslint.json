{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "trailing-comma": [
      false
    ],
    "no-console": [
      true,
      {
        "allow": [
          "warn",
          "error",
          "info"
        ]
      }
    ],
    "no-var-requires": [
      false
    ]
  },
  "rulesDirectory": []
}